{
	"$schema": "http://json.schemastore.org/prettierrc",
	"plugins": ["prettier-plugin-packagejson"],
	"overrides": [
		{
			"files": ".*rc",
			"options": { "parser": "json" }
		},
		{
			"files": ".nvmrc",
			"options": { "parser": "yaml" }
		}
	],
	"useTabs": true,
	"printWidth": 140,
	"semi": false
}
