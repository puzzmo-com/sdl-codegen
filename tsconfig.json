{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"module": "Node16",
		"moduleResolution": "node16",
		"outDir": "lib",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2021"
	},
	"exclude": ["node_modules", "src/tests"],
	"include": ["src"]
}
